{
  "name": "convert-cart/analytics",
  "description": "Magento 2 plugin to integrate ConvertCart analytics",
  "type": "magento2-module",
  "version": "1.0.12",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "php": "^7.0 || ^8.0",
    "magento/framework": "^103.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Convertcart\\Analytics\\": ""
    }
  },
  "extra": {
    "map": [
      [
        "*",
        "Convertcart/Analytics"
      ]
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
